{
  "name": "react-europe",
  "version": "1.1.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "gitbook serve gitbook docs",
    "build:ebook": "gitbook epub gitbook ebook/react-europe.epub",
